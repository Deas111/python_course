# Основные операции
a = 10
b = 3

print(a + b)   # Сложение: 13
print(a - b)   # Вычитание: 7
print(a * b)   # Умножение: 30
print(a / b)   # Деление (всегда float): 3.3333333333333335
print(a // b)  # Целочисленное деление: 3 (отбрасывает дробную часть)
print(a % b)   # Остаток от деления: 1
print(a ** b)  # Возведение в степень: 1000 (10³)